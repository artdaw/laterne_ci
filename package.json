  "repository": {
    "type": "git",
    "url": "git+https://github.com/artdaw/laterne_ci.git"
  },
  "keywords": [
    "ci",
    "lighthouse",
    "docker",
    "cloudwatch"
  ],
  "author": "Gleb Galkin <artdaw@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdaw/laterne_ci/issues"
  },
  "homepage": "https://github.com/artdaw/laterne_ci#readme",
  "dependencies": {
    "aws-sdk": "^2.270.1",
    "lighthouse": "7.0.0"
  },
  "devDependencies": {
    "eslint": "^5.1.0",
    "husky": "^0.14.3",
    "node-prune": "^1.0.2"
  }
}
